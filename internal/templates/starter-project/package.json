{
  "name": "{{.ProjectName}}",
  "version": "1.0.0",
  "private": true,
  "description": "OpenCore server project",
  "packageManager": "{{.PackageManager}}",
  "workspaces": [
    "core",
    "core/*/nui",
    "resources/*",
    "resources/*/ui",
    "resources/*/nui",
    "standalones/*",
    "views/*"
  ],
  "scripts": {
    "dev": "opencore dev",
    "build": "opencore build"
  },
  "devDependencies": {
    "@open-core/cli": "latest",
    "typescript": "^5.3.3",
    "esbuild": "^0.27.2",
    "@swc/core": "^1.15.7",
    "tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "@citizenfx/client": "2.0.22443-1",
    "@citizenfx/server": "2.0.22443-1",
    "reflect-metadata": "^0.2.2",
    "@open-core/framework": "latest"{{if .InstallIdentity}},
    "@open-core/identity": "latest"{{end}}
  }
}